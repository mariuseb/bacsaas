{
    "model": {
        "ode": {
            "dTi": "(Te - Ti)/(Rie*Ci) + phi_h*(1/Ci) + Ai*(phi_s/Ci)",
            "dTe": "(Ti - Te)/(Rie*Ce) + (Ta - Te)/(Rea*Ce)"
        },
        "p": [
            "Rie",
            "Rea",
            "Ci",
            "Ce",
            "Ai"
        ],
        "u": [
            "phi_h",
            "Ta", 
            "phi_s"
        ],
        "x": [
            "Ti",
            "Te"
        ],
        "y": [
            ["y1", "Ti"]
        ]
    },
    "method": "multiple_shooting",
    "integrator": 
        {
        "name": "IRK",
        "method": "legendre",
        "degree": 3,
        "n_steps": 1
        }
    ,
    "solver": "gauss-newton",
    "opt": {
        "verbose": false,
        "ipopt.linear_solver": "ma27",
        "ipopt.ma57_pre_alloc": 3
    },
    "codegen": true
}
